# тестирование функции join

test = [['Партия № 3149'], ['Партия № 2988', 'Партия № 3326'], ['Партия № 2989', 'Партия № 3326'], ['Партия № 3299'],
        ['Партия № 3127', 'Партия № 3233']]

print(type(test))

print(' '.join(str(el) for el in test))
print(' '.join(map(str, test)))

test_2 = ['Партия № 2988', 'Партия № 3326']
print(' '.join(test_2))
